{
  "name": "@qlever-llc/zendesk-sync",
  "version": "0.2.0",
  "description": "Sync zendesk tickets to trellis",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "src/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "start": "node ${INSPECT:-} --enable-source-maps dist/index.js",
    "test": "tsc -b && c8 ava",
    "test:debug": "ava -T 60m -svc 1 --no-worker-threads",
    "lint": "eslint .",
    "template:dev": "cd template && vite dev",
    "template:build": "cd template && vite build",
    "template:preview": "cd template && vite preview",
    "template:check": "cd template && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "template:check:watch": "cd template && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "ava": {
    "failFast": false,
    "files": [
      "**/*.test.ts"
    ],
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "src/": "dist/",
        "test/": ".test/"
      },
      "compile": false
    }
  },
  "c8": {
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true,
    "src": "src",
    "exclude": [
      "*.d.ts",
      ".pnp.*",
      ".test"
    ]
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@oada/client": "^5.0.0",
    "@oada/jobs": "^4.5.1",
    "@oada/lib-config": "^3.9.1",
    "@oada/lib-prom": "^3.8.0",
    "@oada/pino-debug": "^3.10.0",
    "@oada/types": "^3.4.5",
    "axios": "^1.6.8",
    "axios-cache-interceptor": "^1.5.2",
    "clone-deep": "^4.0.1",
    "cron": "^3.1.7",
    "debug": "^4.3.4",
    "es-main": "^1.3.0",
    "md5": "^2.3.0",
    "p-queue": "^8.0.1",
    "p-throttle": "^6.1.0",
    "puppeteer": "^22.7.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@tailwindcss/typography": "^0.5.12",
    "@tsconfig/node16": "^16.1.3",
    "@types/clone-deep": "^4.0.4",
    "@types/convict": "^6.1.6",
    "@types/cron": "^2.4.0",
    "@types/debug": "^4.1.12",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@yarnpkg/sdks": "^3.1.0",
    "autoprefixer": "^10.4.19",
    "ava": "6.1.2",
    "c8": "^9.1.0",
    "daisyui": "^4.10.2",
    "date-fns": "^3.6.0",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-xo": "^0.44.0",
    "eslint-config-xo-typescript": "^4.0.0",
    "eslint-plugin-array-func": "^5.0.1",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-escompat": "^3.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-i18n-text": "^1.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-no-secrets": "^0.9.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-regexp": "^2.5.0",
    "eslint-plugin-security": "^3.0.0",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-unicorn": "^52.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "pretty-bytes": "^6.1.1",
    "svelte": "^4.2.15",
    "svelte-check": "^3.6.9",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.10"
  },
  "packageManager": "yarn@4.2.1",
  "volta": {
    "node": "16.19.0"
  }
}
