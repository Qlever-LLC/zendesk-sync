{
  "name": "@qlever-llc/zendesk-sync",
  "version": "0.1.6",
  "description": "Sync zendesk tickets to trellis",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "src/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "start": "node ${INSPECT:-} --enable-source-maps dist/index.js",
    "test": "tsc -b && c8 ava",
    "test:debug": "ava -T 60m -svc 1 --no-worker-threads",
    "lint": "eslint .",
    "tempalte:dev": "cd template && vite dev",
    "template:build": "cd template && vite build",
    "template:preview": "cd template && vite preview",
    "template:check": "cd template && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "template:check:watch": "cd template && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "ava": {
    "failFast": false,
    "files": [
      "**/*.test.ts"
    ],
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "src/": "dist/",
        "test/": ".test/"
      },
      "compile": false
    }
  },
  "c8": {
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true,
    "src": "src",
    "exclude": [
      "*.d.ts",
      ".pnp.*",
      ".test"
    ]
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@oada/client": "^4.5.5",
    "@oada/lib-config": "^3.9.1",
    "@oada/lib-prom": "^3.8.0",
    "@oada/pino-debug": "^3.9.1",
    "@oada/types": "^3.5.2",
    "axios": "^1.4.0",
    "clone-deep": "^4.0.1",
    "cron": "^2.3.1",
    "debug": "^4.3.4",
    "es-main": "^1.2.0",
    "md5": "^2.3.0",
    "p-queue": "^7.3.4",
    "p-throttle": "^5.1.0",
    "puppeteer": "^20.8.2",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.22.3",
    "@tailwindcss/typography": "^0.5.9",
    "@tsconfig/node16": "^16.1.0",
    "@types/clone-deep": "^4",
    "@types/convict": "^6.1.3",
    "@types/cron": "^2.0.1",
    "@types/debug": "^4.1.8",
    "@types/md5": "^2.3.2",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@yarnpkg/sdks": "^3.0.0-rc.48",
    "autoprefixer": "^10.4.14",
    "ava": "5.3.1",
    "c8": "^8.0.0",
    "daisyui": "^3.5.0",
    "date-fns": "^2.30.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^1.0.0",
    "eslint-plugin-array-func": "^3.1.8",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-escompat": "^3.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.9.0",
    "eslint-plugin-i18n-text": "^1.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-regexp": "^1.15.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^48.0.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "pretty-bytes": "^6.1.1",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.6",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  },
  "packageManager": "yarn@3.6.1",
  "volta": {
    "node": "16.19.0"
  }
}