{
  "search.exclude": {
    ".dccache": true,
    ".history": true,
    ".nvimlog": true,
    "**/node_modules": true,
    "package.lock": true,
    "*.log": true,
    ".DS_Store": true,
    "**/dist": true,
    "**/.test": true,
    "*.tsbuildinfo": true,
    ".actrc": true,
    "act": true,
    "**/.vim": true,
    "**/.yarn": true,
    "**/tags.*": true,
    "**/.pnp.*": true,
    "**/*.tsbuildinfo": true,
    "**/coverage": true
  },
  "prettier.prettierPath": ".yarn/sdks/prettier/index.cjs",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "eslint.nodePath": ".yarn/sdks",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/tags": true,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true,
    ".dccache": true,
    ".history": true,
    ".nvimlog": true,
    "**/node_modules": true,
    "package.lock": true,
    "*.log": true,
    ".DS_Store": true,
    "**/dist": true,
    "**/.test": true,
    "*.tsbuildinfo": true,
    ".actrc": true,
    "act": true,
    "**/.yarn": true,
    "**/.pnp.*": true,
    "**/.vim": true,
    "**/tags.*": true,
    "**/*.tsbuildinfo": true,
    "**/coverage": true
  },
  "licenser.author": "Qlever LLC",
  "licenser.license": "AL2",
  "cSpell.words": [
    "Buildx",
    "centricity",
    "codeql",
    "FSQA",
    "lcov",
    "Logiq",
    "masterid",
    "noreferrer",
    "pdfs",
    "pino",
    "Snyk",
    "sonarjs",
    "trellisfw",
    "Uniquify",
    "unversioned",
    "vdoc"
  ],
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.useESLintClass": true,
  "npm.packageManager": "yarn",
  "debug.javascript.autoAttachFilter": "smart",
  "debug.javascript.breakOnConditionalError": true,
  "debug.javascript.defaultRuntimeExecutable": {
    "node": "yarn",
    "pwa-node": "yarn"
  },
  "debug.javascript.autoAttachSmartPattern": [
    "${workspaceFolder}/**",
    "!**/node_modules/**",
    "**/$KNOWN_TOOLS$/**",
    "!**/yarn-*",
    "!**/ava-*/**/cli.mjs"
  ],
  "explorerExclude.backup": {}
}
